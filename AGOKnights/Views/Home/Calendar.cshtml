        <div class="jumbotron">
            <h1>@Page.Title</h1>
            <div class="hidden-sm hidden-xs"> 
                <iframe src="https://calendar.google.com/calendar/embed?height=600&amp;wkst=1&amp;bgcolor=%23cc0000&amp;src=alchemist%40agoknights.com&amp;color=%23A32929&amp;ctz=America%2FChicago" style="border-width:0;display:block;margin:auto" width="800" height="600" frameborder="0" scrolling="no"></iframe>
            </div>
